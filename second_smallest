#include <iostream>
using namespace std;

int main()
{
    int min1,min2,temp;
    if(cin>>min1)
        min2=min1;//initially,min1 and min2 are the same
    
    while(cin>>temp){
        if(temp>min1 && temp<=min2)
            min2=temp;
        if (temp<=min1)
            min1=temp;
    }
    
    cout<<"the second smallest:"<<min2;

    return 0;
}
